{
  "providers": {
    "openrouter": {
      "name": "OpenRouter",
      "base_url": "https://openrouter.ai/api/v1",
      "api_key": "${OPENROUTER_API_KEY}",
      "models": {
        "gpt-4": "openai/gpt-4",
        "gpt-5": "openai/gpt-5",
        "claude-3": "anthropic/claude-3-opus",
        "default": "openai/gpt-5"
      }
    },
    "aliyun": {
      "name": "Aliyun DashScope",
      "base_url": "https://dashscope.aliyuncs.com/compatible-mode/v1",
